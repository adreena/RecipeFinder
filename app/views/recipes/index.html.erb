<p>Powered By Food2Fork.com</p>
<h1> Searching for <%= @search %> </h1>

<table border="1">
	<tr>
		<th>Image</th>
		<th>Title</th>
		<th>Social Rank</th>
	</tr>
	<% @recipes.each do |recipe| %>
		<tr>
			<td><%= image_tag(recipe["image_url"]) %> </td>
			<td><%= recipe["title"] %> </td>
			<td><%= recipe["social_rank"] %> </td>
		</tr>
	<% end %>
</table>

